program dance {
	
	do RobotControl.setProperties( "RKP RKP LKP LKP RAP RAP LAP LAP", 
									"velocity position velocity position velocity position velocity position"
									".5 1.2 .5 1.2 .5 -.77 .5 -.77");
									
	

	while (RobotControl.getProperty("RKP", "position") != 1.2){
		do nothing
	} 

	while (RobotControl.getProperty("LKP", "position") != 1.2){
		do nothing
	}
	
	while (RobotControl.getProperty("RAP", "position") != -.77){
		do nothing
	}

	while (RobotControl.getProperty("LAP", "position") != -.77){
		do nothing
	}
	
	do RobotControl.setProperties( "RKP LKP RAP LAP", 
									"position position position position"
									"0 0 0 0");
	
}
