path("/opt/ros/fuerte/stacks/maestro")
import("maestro")

program gesture {
	do RobotControl.setRightHipYaw(90000,0)
	
	do 
		nothing 
	until {
		if not RobotControl.requiresMotion(0, 0, 0) then continue
	}
	
	do RobotControl.setRightHipYaw(-90000,0)
	
	do 
		nothing 
	until {
		if not RobotControl.requiresMotion(0, 0, 0) then continue
	}
	
	
}