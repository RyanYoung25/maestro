//import("RobotControl")

program gesture{
	try RobotControl.setRightHipYaw(90000,0)

	while (true){
		if RobotControl.requiresMotion(0, 0, 0) then 
			break
	}
	
	do RobotControl.setRightHipYaw(-90000,0)
	
	while (true) {
		if RobotControl.requiresMotion(0, 0, 0) then
			break
	}
	
	do RobotControl.setRightHipYaw(0, 0)
}
