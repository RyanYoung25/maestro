program gesture
{
	do RobotControl.debugControl(0, speed,5)
	do RobotControl.setRightHipYaw(60000, speed,0)

	while (RobotControl.requiresMotion(0,0,0)){
		do nothing
	}	

	do RobotControl.setRightHipYaw(0, speed,0)
	while (RobotControl.requiresMotion(0,0,0)){
		do nothing
	}
	do RobotControl.debugControl(0, speed,6)
	
}
